    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "mitmproxy",
            "type": "python",
            "request": "launch",
            "program": "./venv/bin/mitmdump",
            "args": [
                "-s",
                "${file}",
                "-e !(~d mp.weixin.qq.com)"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {},
            "stopOnEntry": false
        }
    ]
}